<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content">
    <ImageView android:id="@+id/imageView1" android:src="@drawable/ic_contact_picture" android:layout_marginBottom="6dip" android:layout_marginTop="6dip" android:adjustViewBounds="true" android:layout_marginLeft="4dip" android:layout_marginRight="14dip" android:layout_width="48dip" android:layout_height="48dip"></ImageView>
    <TableLayout android:layout_width="fill_parent" android:id="@+id/tableLayout2" android:layout_height="wrap_content">
        <TextView android:layout_width="wrap_content" android:text="Name" android:layout_height="wrap_content" android:textSize="24dip" android:id="@+id/name"></TextView>
        <TextView android:layout_width="wrap_content" android:text="PhoneticName" android:textSize="16dip" android:layout_height="wrap_content" android:id="@+id/phoneticName"></TextView>
    </TableLayout>
</LinearLayout>
